model/tutorials/**/data/*
model/examples/**/*.pdf
model/examples/**/*.json
model/examples/**/data/*
model/motivations/**/*.pdf
model/motivations/**/*.json
model/motivations/**/data/*

# Ignore documentation files
model/doc/_build/*
model/doc/auto_examples/*
model/doc/auto_tutorials/*
model/doc/auto_motivations/*
model/doc/generated/*
